{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test DB_DATABASE=guacamoleTestDb PORT=5036 mocha \"test/**/*.test.js\" --exit",
    "start": "node .",
    "start:watch": "nodemon .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "import",
      "promise"
    ],
    "rules": {
      "promise/catch-or-return": "error",
      "no-console": "off",
      "array-bracket-newline": "warn",
      "array-bracket-spacing": "warn",
      "no-unused-vars": "warn",
      "camelcase": "warn",
      "eqeqeq": "error",
      "no-duplicate-imports": "error",
      "no-else-return": "error",
      "no-useless-rename": "error",
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "no-unneeded-ternary": "error",
      "import/order": [
        "warn",
        {
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          },
          "newlines-between": "always-and-inside-groups"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "test/**"
        ],
        "rules": {
          "promise/catch-or-return": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.2",
    "autodetect-decoder-stream": "^1.0.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "csv-reader": "^1.0.7",
    "dateformat": "^4.5.1",
    "deep-equal-in-any-order": "^1.0.28",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.5.0",
    "js-levenshtein": "^1.1.6",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.3",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-cron": "^2.0.3",
    "request": "^2.88.2",
    "sinon": "^9.2.4",
    "supertest": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2"
  }
}
